<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!-- 
  
  DO NOT EDIT THIS FILE DIRECTLY 
  
  Add a local.config file to prevent settings from getting committed
  
  See https://msdn.microsoft.com/en-us/library/aa903313%28v=vs.71%29.aspx 
  for more information
  
  -->
  <appSettings file="local.config">
    <!-- Fetch api token from Configured Integrations > Bots > bot > API token -->
    <add key="slack-api-key" value="FIXME"/>
    <!-- A Redgate Id account in the form user@example.com:password -->
    <add key="redgate-id" value="FIXME"/>
    <!-- A url for the teamcity api in the form http://foo:2001/ -->
    <add key="teamcity-api-base" value="FIXME"/>
    <!-- 
    An endpoint url for the tcWebHooks plugin 
    
    Depending on the endpoint, you may need to run this as admin to fix Access Denied errors:
    netsh http add urlacl url=http://*:51307/ user=DOMAIN\user
    -->
    <add key="teamcity-webhooks-endpoint" value="http://*:51307/"/>
    <!-- A github OAUTH token -->
    <add key="github-token" value="FIXME" />
    <!-- The default repo for talking to github -->
    <add key="github-default-repo" value="FIXME"/>
    <!-- The default user for talking to github -->
    <add key="github-default-user" value="FIXME"/>
    <!-- Logging dashboard for seeing what's going on -->
    <add key="logging-dashboard-endpoint" value="http://localhost:51308/"/>
    <!-- auth for rg webcams -->
    <add key="webcam-auth" value="FIXME"/>
    <!-- auth for teamcity -->
    <add key="teamcity-auth" value="FIXME"/>
  </appSettings>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>